# For rails part
################

# Ignore bundler config.
/rails/.bundle

# Ignore the default SQLite database.
/rails/db/*.sqlite3
/rails/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/rails/log/*.log
/rails/tmp


# For ember part
################

# compiled output
/ember/dist
/ember/tmp

# dependencies
/ember/node_modules
/ember/bower_components/*

# misc
/ember/.sass-cache
/ember/connect.lock
/ember/coverage/*
/ember/libpeerconnection.log
npm-debug.log
testem.log